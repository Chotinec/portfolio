mixin socials(mod) 
  .socials
    ul.socials__list
      - var socials = ['insta', 'vk', 'github']
      each social in socials
        li(class="socials__item socials__item--" + mod)
          a(class='socials__link ' href='#')
            +icon(social, `socials__icon socials__icon--${mod} socials__icon--${social}`)